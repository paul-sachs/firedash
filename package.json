{
  "name": "reforge-boilerplate",
  "version": "0.0.2",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/influitive/reforge-boilerplate.git"
  },
  "scripts": {
    "test": "BABEL_ENV=test babel-tape-runner src/**/*.test.js | tap-spec",
    "test:watch": "nodemon --quiet --exec \"npm run --silent test\" --watch src",
    "start": "./scripts/start.sh",
    "build": "rm -rf build && kotatsu build client -c config/webpack.config.js --babel --quiet",
    "lint": "eslint .",
    "lint:watch": "nodemon --quiet --exec \"npm run --silent lint\" --watch src"
  },
  "pre-commit": {
    "run": "test, lint"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-cli": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-tape-runner": "^2.0.1",
    "commitizen": "^2.6.1",
    "cz-conventional-changelog": "^1.1.5",
    "deep-freeze": "0.0.1",
    "gild": "0.0.6",
    "jss": "^3.3.0",
    "kotatsu": "^0.13.0",
    "lodash": "^4.15.0",
    "nodemon": "^1.9.1",
    "postcss-simple-vars": "^1.2.0",
    "provide-map": "^1.0.2",
    "react": "^15.3.0",
    "react-container-query": "^0.4.0",
    "react-dom": "^15.3.0",
    "react-grid-layout": "^0.13.1",
    "react-redux-provide": "^6.0.0-rc.14",
    "recompose": "^0.20.2",
    "redux": "^3.3.1",
    "redux-act": "^0.5.0",
    "redux-saga": "^0.10.0",
    "redux-thunk": "^2.0.1",
    "reforge": "^1.0.1",
    "reselect": "^2.1.0",
    "tap-dot": "^1.0.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.0",
    "tape-catch": "^1.0.4",
    "webpack-merge": "^0.8.3"
  },
  "author": "",
